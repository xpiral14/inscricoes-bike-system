{
    "listeners": {
        "*:3000": {
            "pass": "routes"
        }
    },
    "routes": [
        {
            "match": {
                "uri": "/"
            },
            "action": {
                "pass": "applications/laravel"
            }
        }
    ],
    "applications": {
        "laravel": {
            "type": "php",
            "root": "/var/www/html",
            "script": "index.php"
        }
    }
}
